program fibonacci
begin
   procedure fib(let k)
   begin
        if (k < 2) then 
            begin 
                return 1; 
            end
        else
            begin 
                return fib(k-1) + fib(k-2); 
            end
    end
    let x := 1;
    let arr[15];
    while (x < 15) do begin
        arr[x] := fib(x);
        x := x + 1;
    end
    x := 1;
    while (x < 15) do begin
        println arr[x];
        x := x + 1;
    end    
end.